<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Azimuth.DataAccess" namespace="Azimuth.DataAccess.Entities">
  <class name="PlaylistListeners" table ="PlaylistListeners">
    <id name="Id">
      <generator class="native"/>
    </id>
    <one-to-one name="Playlist" class="Playlist" column="PlaylistId" cascade="all" constrained="true" fetch="select"/>
    <propery name="Amount"></propery>
  </class>
</hibernate-mapping>
